<div>
    <h1>{{type}}</h1>
    <ul class="asset-view">
        {{#each assetts}}
        <li>
            <p>{{type}}</p>
        </li>
        <li>
            <p>{{host_name}}</p>
        </li>
        <li>
            <p>{{ip_address}}</p>
        </li>
        <li>
            <p>{{subnet_mask}}</p>
        </li>
        <li>
            <p>{{default_gateway}}</p>
        </li>
        <li>
            <img src="{{image}}" alt="{{type}}">
        </li>
        <li>
            <p>{{live}}</p>
        </li>
        {{/each}}
    </ul>
</div>